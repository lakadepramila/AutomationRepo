package loops;

public class NestedForLoop {

	public static void main(String[] args) {
		//initialization //conditional // Increment or Decrement
				for (int r=1;r<=4;r++)//you can use debug for step by step execution
				{	
					for(int c=1;c<=2;c++){
						System.out.print("Row Num.="+r+"col no.="+c+" ");
					}
					System.out.println();
				}


	}

}
